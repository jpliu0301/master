<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         id="application"
         point="org.eclipse.core.runtime.applications">
      <application
            icon="icons/replication_title.png">
         <run
               class="com.kingbase.db.replication.application.intro.Application">
         </run>
      </application>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="com.kingbase.db.replication.application.intro.Perspective"
            icon="icons/replication_title.png"
            id="com.kingbase.db.replication.application.perspective"
            name="RCP Perspective">
      </perspective>
   </extension>
   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="com.kingbase.db.tools.application.application"
            name="@@TOOL_NAME@@">
         <property
               name="appName"
               value="@@TOOL_NAME@@">
         </property>
         <property
               name="windowImages"
               value="icons/replication_title.png">
         </property>
         <property
               name="startupForegroundColor"
               value="FF0000">
         </property>
         <property
               name="startupMessageRect"
               value="7,252,445,20">
         </property>
         <property
               name="startupProgressRect"
               value="5,275,445,15">
         </property>
         <property
               name="preferenceCustomization"
               value="plugin_customization.ini">
         </property>
         <property
                name="aboutText"
               value="%productAbout">
         </property>
         <property
               name="aboutImage"
               value="icons/about.png">
         </property>
      </product>
   </extension>
	<extension
          point="org.eclipse.ui.intro">
       <intro
             class="org.eclipse.ui.intro.config.CustomizableIntroPart"
             icon="icons/dbeaver16.png"
             id="com.kingbase.tool.intro"
             label="欢迎">
       </intro>
       <introProductBinding
             introId="com.kingbase.tool.intro"
             productId="com.kingbase.db.tools.application.product">
       </introProductBinding>
    </extension>
	<extension
          point="org.eclipse.ui.intro.config">
       <config
             content="welcome/introContent.xml"
             id="org.jkiss.dbeaver.core.application.config"
             introId="com.kingbase.tool.intro">
          <presentation
                home-page-id="root">
             <implementation
                   kind="html"
                   os="win32,linux,macosx">
             </implementation>
          </presentation>
       </config>
    </extension>
</plugin>
